    <children>
        <Button fx:id="resetPasswordButton" layoutX="248.0" layoutY="208.0" mnemonicParsing="false" onAction="#resetPassword" text="Reset" />
        <PasswordField fx:id="newPasswordField" layoutX="213.0" layoutY="83.0" promptText="newPasswordField" />
        <PasswordField fx:id="confirmPasswordField" layoutX="214.0" layoutY="139.0" promptText="confirmPasswordField" />
      <PasswordField fx:id="code" layoutX="205.0" layoutY="50.0" promptText="code" />
    </children>
</AnchorPane>
